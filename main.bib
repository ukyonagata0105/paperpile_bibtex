  year      =  2020,
  url       = "https://www.sciencedirect.com/science/article/pii/S0965856418309601",
  file      = "Other/Pangbourne et al. 2020 - Questioning mobility as a service - Unanticipated implications for society and governance.pdf",
  keywords  = "Smart mobility; Mobility as a service; Governance; Equity;
               Technological transition",
  doi       = "10.1016/j.tra.2019.09.033",
  issn      = "0965-8564,1879-2375",
  language  = "en"
}

@ARTICLE{Mladenovic2021-fw,
